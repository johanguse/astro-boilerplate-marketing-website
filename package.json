    "@resvg/resvg-js": "^2.6.2",
    "@tailwindcss/vite": "^4.1.18",
    "@xenova/transformers": "^2.17.2",
    "astro": "^5.17.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lodash.kebabcase": "^4.1.1",
    "lucide-react": "^0.563.0",
    "luxon": "^3.7.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "remark-collapse": "^0.1.2",
