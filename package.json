{
  "name": "astro-boilerplate-i18n",
  "version": "2.0.0",
  "type": "module",
  "private": false,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && pagefind --site dist && cp -r dist/pagefind public/",
    "build:full": "bun run generate:similarities && bun run build",
    "generate:similarities": "bun run scripts/generate-similarities.ts",
    "deploy:cf": "wrangler deploy --env production",
    "preview:cf": "bun run build && wrangler dev",
    "cf:typegen": "wrangler typegen --env production",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "biome check .",
    "format": "biome check --write .",
    "lint": "biome lint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "type-check": "astro check"
  },
  "volta": {
    "node": "20.19.0",
    "pnpm": "10.6.5"
  },
  "engines": {
    "node": "20.19.0"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "^4.0.15",
    "@astrojs/sitemap": "^3.7.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
