    "@types/luxon": "^3.7.1",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "~3.0.9",
    "pagefind": "^1.4.0",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
