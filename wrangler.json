{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "astro-boilerplate-i18n",
  "compatibility_date": "2026-01-28",
  "placement": {
    "mode": "smart",
  },
  "observability": {
    "enabled": true
  },
  "main": "dist/_worker.js/index.js",
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": "dist"
  },
  "kv_namespaces": [
    {
      "binding": "SESSION",
      "id": "placeholder",
      "preview_id": "placeholder"
    }
  ]
}